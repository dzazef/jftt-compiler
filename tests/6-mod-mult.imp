[ a ^ b mod c 
? 1234567890
? 1234567890987654321
? 987654321
> 674106858
]
DECLARE
    a, b, c, wynik, pot, wybor
BEGIN
    READ a;
    READ b;
    READ c;
    wynik ASSIGN 1;
    pot ASSIGN a MOD c;
    WHILE b GE 0 DO
        WRITE 11111111;
        WRITE 22222222;
		wybor ASSIGN b MOD 2;
		WRITE 33333333;
		WRITE 44444444;
		IF wybor EQ 1 THEN
		    WRITE 55555555;
		    WRITE 55555555;
		    WRITE 55555555;
		    WRITE 55555555;
		    WRITE 55555555;
		    WRITE 55555555;
		    WRITE 55555555;
			wynik ASSIGN wynik TIMES pot;
		    WRITE 51111111;
		    WRITE 51111111;
		    WRITE 51111111;
		    WRITE 51111111;
		    WRITE 51111111;
		    WRITE 51111111;
			wynik ASSIGN wynik MOD c;
		    WRITE 52222222;
		ENDIF
		WRITE 66666666;
		WRITE 77777777;
		WRITE 88888888;
		b ASSIGN b DIV 2;
		pot ASSIGN pot TIMES pot;
		pot ASSIGN pot MOD c;
		WRITE 99999999;
    ENDWHILE
    WRITE wynik;
END
